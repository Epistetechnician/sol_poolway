{
  "name": "solana-dashboard",
  "version": "1.0.0",
  "description": "Solana Orca pools analyzer and dashboard",
  "main": "dist/index.js",
  "scripts": {
    "build": "tsc",
    "start": "node dist/monitor.js",
    "dev": "ts-node src/monitor.ts",
    "test": "ts-node src/tests/orca.test.ts",
    "test:ticks": "ts-node src/scripts/test_tick_fetching.ts",
    "lint": "eslint . --ext .ts",
    "format": "prettier --write \"src/**/*.ts\"",
    "monitor": "ts-node src/monitor.ts",
    "watch": "ts-node-dev --respawn src/monitor.ts",
    "postinstall": "npm run build"
  },
  "dependencies": {
    "@coral-xyz/anchor": "^0.31.0",
    "@orca-so/sdk": "^1.2.26",
    "@orca-so/whirlpools": "^1.1.0",
    "@orca-so/whirlpools-core": "^1.0.4",
    "@orca-so/whirlpools-sdk": "^0.11.9",
    "@project-serum/anchor": "^0.26.0",
    "@solana/kit": "^2.1.0",
    "@solana/web3.js": "^1.98.0",
    "@types/decimal.js": "^0.0.32",
    "@types/node": "^20.17.24",
    "@types/pg-promise": "^5.4.3",
    "axios": "^1.6.7",
    "decimal.js": "^10.5.0",
    "dotenv": "^16.4.5",
    "pg": "^8.11.3",
    "pg-promise": "^11.5.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.8.2",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/pg": "^8.11.2",
    "@types/winston": "^2.4.4",
    "@typescript-eslint/eslint-plugin": "^7.1.1",
    "@typescript-eslint/parser": "^7.1.1",
    "eslint": "^8.57.0",
    "jest": "^29.7.0",
    "prettier": "^3.2.5",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0"
  },
  "engines": {
    "node": ">=16.0.0"
  }
}